{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/index.js"
      },
      {
        "src": "/(.*)",
        "headers": { "key": "Access-Control-Allow-Credentials", "value": "true" }
      }
    ]
  }
  